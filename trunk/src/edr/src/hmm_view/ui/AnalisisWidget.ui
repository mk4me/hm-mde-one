}

/* timeline */
#analisisArea .QMainWindow .QTreeWidget {
	background: transparent;
}

/* topWindow i bottomWindow */
#analisisArea &gt; QMainWindow {
	border: 1px solid black;
	border-radius: 4px;
	background: white;
}

/* topWindow */
#analisisArea &gt; EDRDockWidgetManager {
	border-bottom-left-radius: 0px;
	border-bottom-right-radius: 0px;
	margin: 0px;
	padding: 0px;
}

#analisisArea &gt; EDRDockWidgetManager QMainWindow {
	border-radius: 0px;
	background: white;
	margin: 0px;
	padding: 0px;
}

VisualizerWidget &gt; .QWidget {
	border-top-left: 0px;
	border-bottom-left: 0px;
}

/* styl titlebar√≥w i ich dzieci */
EDRTitleBar, EDRTitleBar * {
	background-color: rgb(43, 42, 41);
	color: white;
	border: none;
	border-radius: 0px;
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	margin: 0px;
	padding: 0px;
}

EDRTitleBar QToolButton#buttonFloat {
    background: rgb(43, 42, 41);
    border: 1px solid rgb(63, 62, 61);
     border-bottom: none;
	 border-radius: 0px;
     border-top-left-radius: 4px;
     border-right: none;
	margin-top: 4px;
	max-height: 12px;
	min-width: 17px;
} 

EDRTitleBar QToolButton#buttonFloat:hover {
    background-color: rgb(135, 177, 255);
 }

EDRTitleBar QToolButton#buttonFloat:pressed {
    background-color: rgb(55, 97, 180);
}

EDRTitleBar QToolButton#buttonClose {
    background-color: rgb(43, 42, 41);
     border: 1px solid rgb(63, 62, 61);
     border-bottom: none; 
	border-radius: 0px;
     border-top-right-radius: 4px;
		margin-top: 4px;
	max-height: 12px;
	min-width: 23px;
}

 EDRTitleBar QToolButton#buttonClose:hover {
     background-color: rgb(200, 0, 0);	
 }

EDRTitleBar QToolButton#buttonClose:pressed {
         background-color: rgb(128, 0, 0);
}

QMainWindow::separator {
	background-color: transparent;
	width: 4px;
	height: 4px;
}

QMainWindow::separator:hover {
	background-color: rgb(190, 210, 250);
}

QMainWindow::separator:vertical {
	image: url(:/resources/icons/splitter-dots-vertical.png);
 }

QMainWindow::separator:horizontal {
	image: url(:/resources/icons/splitter-dots-horizontal.png);
}

QMainWindow::separator:vertical:hover {
	margin-top: 4px;
	margin-bottom: 4px;
 }

QMainWindow::separator:horizontal:hover {
	margin-left: 4px;
	margin-right: 4px;
}

QTabBar, QTabBar * {}
QTabBar {
	background: rgb(91, 91, 91);
	border-top: 3px solid black;
}

QTabBar::tab {
	border: 1px solid  black;
	border-top: 3px solid black;
	border-radius: 0px;
	background-color: transparent;
	color: white;
	min-width: 10ex;
	padding: 2px;
	margin: 0px;
}

QTabBar::tab:previous-selected {
	border-right: none;
}

QTabBar::tab:next-selected, QTabBar::tab:middle:next-selected {
	border-right: 1px solid black;
}

QTabBar::tab:first {
	border: 1px solid  black;
	border-top: 3px solid black;
	border-bottom-left-radius: 4px;
}

QTabBar::tab:first:previous-selected {
	border-right: none;
}

QTabBar::tab:middle {
	border: 1px solid  black;
	border-top: 3px solid black;
	border-left: none;
}

QTabBar::tab:last {
	border: 1px solid  black;
	border-top: 3px solid black;
	border-left: none;
	border-bottom-right-radius: 4px;
}

QTabBar::tab:middle:selected {
	border: 1px solid  black;
	border-top: 3px solid black;
	background-color: black;
	border-bottom-left-radius: 0px;
	border-bottom-right-radius: 0px;
	margin-left: -1px;
	margin-right: -1px;
}

QTabBar::tab:first:selected {
border: 1px solid black;
	border-top: 3px solid black;
	background-color: black;
	border-bottom-left-radius: 4px;
	border-bottom-right-radius: 0px;
	margin-left: 0px;
	margin-right: -1px;
}

QTabBar::tab:last:selected {
border: 1px solid  black;
border-top: 3px solid black;
	background-color: black;
	border-bottom-left-radius: 0px;
	border-bottom-right-radius: 4px;
		margin-left: -1px;
	margin-right: 0px;
}

